<?xml version="1.0" encoding="UTF-8"?>
<facelet-taglib xmlns="http://java.sun.com/xml/ns/javaee"
                xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd"
                version="2.0">
    <namespace>urn:quadro:jsf:dynatree-for-jsf</namespace>


    <tag>
        <tag-name>dynatree</tag-name>
        <component>
            <component-type>org.quadro.jsf.component.dynatree.DTreeView</component-type>
        </component>

        <attribute>
            <description>Unique identifier of the component in a namingContainer</description>
            <name>id</name>
            <required>false</required>
            <type>java.lang.String</type>
        </attribute>

        <attribute>
            <description>Init tree structure. It can be a String containing the JSON or a collection converted to JSON.</description>
            <name>children</name>
            <required>false</required>
            <type>java.lang.Object</type>
        </attribute>

        <attribute>
            <description>Tree additionnal options. It can be a String containing the JSON or a map converted to JSON</description>
            <name>options</name>
            <required>false</required>
            <type>java.lang.Object</type>
        </attribute>


        <attribute>
            <description>Javascript widget variable.</description>
            <name>widgetVar</name>
            <required>false</required>
            <type>java.lang.String</type>
        </attribute>


        <attribute>
            <description>CSS style(s) to be applied when this component is rendered.</description>
            <name>styleClass</name>
            <required>false</required>
            <type>java.lang.String</type>
        </attribute>

        <attribute>
            <description>Space-separated list of CSS style class(es) to be applied when this element is rendered.
                This value must be passed through as the "class" property on generated markup.
            </description>
            <name>style</name>
            <required>false</required>
            <type>java.lang.String</type>
        </attribute>

        <!-- For a complete description of these events, you can open the dynatree documentation
        http://wwwendt.de/tech/dynatree/doc/dynatree-doc.html  -->
        <attribute>
            <description></description>
            <name>onClick</name>
            <required>false</required>
            <type>java.lang.Object</type>
        </attribute>

        <attribute>
            <description></description>
            <name>onDblClick</name>
            <required>false</required>
            <type>java.lang.Object</type>
        </attribute>

        <attribute>
            <description></description>
            <name>onKeydown</name>
            <required>false</required>
            <type>java.lang.Object</type>
        </attribute>

        <attribute>
            <description></description>
            <name>onKeypress</name>
            <required>false</required>
            <type>java.lang.Object</type>
        </attribute>

        <attribute>
            <description></description>
            <name>onFocus</name>
            <required>false</required>
            <type>java.lang.Object</type>
        </attribute>

        <attribute>
            <description></description>
            <name>onBlur</name>
            <required>false</required>
            <type>java.lang.Object</type>
        </attribute>

        <attribute>
            <description></description>
            <name>onQueryActivate</name>
            <required>false</required>
            <type>java.lang.Object</type>
        </attribute>

        <attribute>
            <description></description>
            <name>onQuerySelect</name>
            <required>false</required>
            <type>java.lang.Object</type>
        </attribute>

        <attribute>
            <description></description>
            <name>onQueryExpand</name>
            <required>false</required>
            <type>java.lang.Object</type>
        </attribute>

        <attribute>
            <description></description>
            <name>onPostInit</name>
            <required>false</required>
            <type>java.lang.Object</type>
        </attribute>

        <attribute>
            <description></description>
            <name>onActivate</name>
            <required>false</required>
            <type>java.lang.Object</type>
        </attribute>

        <attribute>
            <description></description>
            <name>onDeactivate</name>
            <required>false</required>
            <type>java.lang.Object</type>
        </attribute>

        <attribute>
            <description></description>
            <name>onSelect</name>
            <required>false</required>
            <type>java.lang.Object</type>
        </attribute>

        <attribute>
            <description></description>
            <name>onExpand</name>
            <required>false</required>
            <type>java.lang.Object</type>
        </attribute>

        <attribute>
            <description></description>
            <name>onLazyRead</name>
            <required>false</required>
            <type>java.lang.Object</type>
        </attribute>

        <attribute>
            <description></description>
            <name>onCustomRender</name>
            <required>false</required>
            <type>java.lang.Object</type>
        </attribute>

        <attribute>
            <description></description>
            <name>onCreate</name>
            <required>false</required>
            <type>java.lang.Object</type>
        </attribute>

        <attribute>
            <description></description>
            <name>onRender</name>
            <required>false</required>
            <type>java.lang.Object</type>
        </attribute>

        <attribute>
            <description></description>
            <name>postProcess</name>
            <required>false</required>
            <type>java.lang.Object</type>
        </attribute>

    </tag>


</facelet-taglib>
